<$list filter="[all[current]links[]] [all[current]backlinks[]] +[first[]]" variable=_>
<div class="linkExplorer">
    <div class="linksIn column">
        <h3>Inbound</h3>
         <<list-links "[all[current]backlinks[]]">>
    </div>
    <div class="linksOut column">
        <h3>Outbound</h3>
        <<list-links "[all[current]links[]]">>
     </div>
</div>
</$list>